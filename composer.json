{
    "name": "jonston/laravel-chat",
    "description": "Laravel Chat Package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonston",
            "email": "jonston@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jonston\\LaravelChat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonston\\LaravelChat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jonston\\LaravelChat\\LaravelChatServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
